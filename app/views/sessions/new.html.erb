<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.label :remember_me do %>
    <%= f.check_box :remember_me %>
    <span>Remember</span>
    <% end %>
  <%= f.submit "Log in" %>
<% end %>